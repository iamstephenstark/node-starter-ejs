{
  "name": "node-starter-ejs",
  "version": "0.0.1",
  "description": "Simple Node starter with nodemon and EJS",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "set DEBUG=app & nodemon app.js",
    "lint": "eslint app.js"
  },
  "author": "Stephen Stark",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "4.1.3",
    "chalk": "2.4.1",
    "debug": "3.1.0",
    "ejs": "2.6.1",
    "express": "4.16.3",
    "jquery": "3.3.1",
    "morgan": "1.9.0",
    "path": "0.12.7"
  },
  "devDependencies": {
    "eslint": "5.5.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "nodemon": "1.18.4"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": ["node_modules/**/node_modules"],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 3000
    }
  }
}
